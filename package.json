{
  "name": "platts",
  "version": "0.0.1",
  "description": "A Dictionary of Urdū, Classical Hindī, and English but as a Service",
  "main": "src/index.js",
  "author": "Moacir P. de Sá Pereira <moacir.p@columbia.edu> (http://moacir.com)",
  "repository": "https://github.com/muziejus/platts.git",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "cheerio": "^1.0.0-rc.3",
    "xo": "^0.25.3"
  },
  "xo": {
    "envs": [
      "amd",
      "browser"
    ],
    "globals": [],
    "prettier": true,
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "space": 2,
    "rules": {
      "require-await": 0,
      "no-unused-expressions": 0,
      "import/extensions": [
        "error",
        "never",
        {
          "graphql": "always"
        }
      ],
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 1
    }
  }
}
